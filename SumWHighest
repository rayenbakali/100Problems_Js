let array = [6, 2, 1, 8, 10];
let empty = [];
let sum = 0;
let max = 0;
let min = 1;
function SumWHL(arr) {
  if (arr == null) return 0;
  else {
    for (let i = 0; i < arr.length; i++) {
      if (max < arr[i]) {
        max = arr[i];
      } else if (min > arr[i]) {
        min = arr[i];
      }
      sum += arr[i];
    }
    console.log(max);
    console.log(min);

    return sum - (max + min);
  }
}

console.log(SumWHL(empty));
